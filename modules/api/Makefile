grpc-python: connection_tracker_api/connection_pb2.py

%_pb2.py: %.proto
	poetry run python -m  grpc_tools.protoc -I./ --python_out=./ --grpc_python_out=./ $<

requirements.txt: pyproject.toml poetry.lock
	echo \# WARNING: do not edit this file manually. Python venv in this project is managed by poetry. > $@
	echo \# requirements.txt is exported only to ensure compatibility. >> $@
	poetry export --without-hashes >> $@